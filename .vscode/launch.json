{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: datasets.py -c",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/dataset/dataset.py",
            "console": "integratedTerminal",
            "args": [
                "-c"
            ]
        },
        {
            "name": "Python Debugger: datasets.py -t",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/dataset/dataset.py",
            "console": "integratedTerminal",
            "args": [
                "-t"
            ]
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: make_trace -p",
            "type": "debugpy",
            "request": "launch",
            "module": "make_trace",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "-p",
                "${workspaceFolder}/tlsf_specs",
                "debug.jsonl",
                "1",
                "1",
                "10"
            ],
            "env": {
                "PATH": "/usr/local/bin:${env:PATH}",
                "LD_LIBRARY_PATH": "/usr/local/lib:${env:LD_LIBRARY_PATH}"
            }
        },
        {
            "name": "Python: make_trace -s",
            "type": "debugpy",
            "request": "launch",
            "module": "make_trace",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "-s",
                "${workspaceFolder}/tlsf_specs/spurious_arbiter.tlsf",
            ],
            "env": {
                "PATH": "/usr/local/bin:${env:PATH}",
                "LD_LIBRARY_PATH": "/usr/local/lib:${env:LD_LIBRARY_PATH}"
            }
        }
    ]
}
